<div class="small-8 small-centered columns">
  <h3 class="text-center">Admins:</h3>
  <% @admins.each do |admin| %>
    <p class="text-center">
      <%= admin.name %>
    </p>
  <% end %>
  <h3 class="text-center">Users:</h3>
  <% @users.each do |user| %>
    <p class="text-center">
      <%= user.name %>
    </p>
  <% end %>
</div>

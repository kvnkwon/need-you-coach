<div class="small-8 small-centered columns">
  <h2 class="text-center">Available</h2>
  <ul class="small-block-grid-3">
    <% unless @coaches.empty? %>
      <% @coaches.each do |coach| %>
        <li><%= coach.name %></li></ul>
      <% end %>
    <% else %>
      <h2 class="text-center">No coaches are available. Sorry!<h2>
    <% end %>
</div>




<h2>All Coaches and Mentors</h2>
<% @all_coaches.each do |coach| %>
  <%= coach.name %><br>
<% end %>

<a href="/">Back to Help Requests</a>

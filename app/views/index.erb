<h2>Help Requests</h2>
<% @user_requests.each do |request| %>
  <a href="/user/<%= request.student_id %>"><%= request.user.name %></a> - <a href="/request/<%= request.id %>"><%= h request.title %></a><br>
<% end %>
<a href="/coaches">
  <% if @coaches.count == 0 %>
    No Coaches Available
  <% elsif @coaches.count == 1 %>
    <%= @coaches.count %> Coach Available
  <% else %>
    <%= @coaches.count %> Coaches Available
  <% end %>
</a><br>
<a href="/request/new">Create a Help Request</a>
